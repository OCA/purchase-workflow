<?xml version="1.0" ?>
<odoo>
    <template
        id="report_purchaseorder_document"
        inherit_id="purchase_order.report_purchaseorder_document"
    >
        <xpath expr="//p[@t-field='doc.incoterm.code']" position="replace">
            <p>
                <span t-field="doc.incoterm.code"/>
                <span t-field="doc.incoterm_place"/>
            </p>
        </xpath>
    </template>
</odoo>
